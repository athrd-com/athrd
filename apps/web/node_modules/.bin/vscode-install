#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gregorymarcilhacy/code/athrd/node_modules/.pnpm/vscode@1.1.37/node_modules/vscode/bin/node_modules:/Users/gregorymarcilhacy/code/athrd/node_modules/.pnpm/vscode@1.1.37/node_modules/vscode/node_modules:/Users/gregorymarcilhacy/code/athrd/node_modules/.pnpm/vscode@1.1.37/node_modules:/Users/gregorymarcilhacy/code/athrd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gregorymarcilhacy/code/athrd/node_modules/.pnpm/vscode@1.1.37/node_modules/vscode/bin/node_modules:/Users/gregorymarcilhacy/code/athrd/node_modules/.pnpm/vscode@1.1.37/node_modules/vscode/node_modules:/Users/gregorymarcilhacy/code/athrd/node_modules/.pnpm/vscode@1.1.37/node_modules:/Users/gregorymarcilhacy/code/athrd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vscode/bin/install" "$@"
else
  exec node  "$basedir/../vscode/bin/install" "$@"
fi
